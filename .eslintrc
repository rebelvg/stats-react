{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/href-no-hash": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": { "config": "webpack.config.js" }
    }
  }
}